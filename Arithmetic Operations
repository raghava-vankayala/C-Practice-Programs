#include <stdio.h>
 
int main()
{
   int a,b,add,sub,mul;
   float div;
 
   printf("Enter two integers A and B:\n");
   scanf("%d%d", &a,&b);
 
   add =a+b;
   sub =a-b;
   mul =a*b;
   div =a/(float)b;   //typecasting
 
   printf("Addition of two values = %d\n", add);
   printf("Subraction of two values = %d\n", sub);
   printf("Multiplication of two values = %d\n", mul);
   printf("Division of two values = %.2f\n", div);
 
   return 0;
}
